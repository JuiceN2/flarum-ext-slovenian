{
    "name": "juicen2/flarum-ext-slovenian",
    "type": "flarum-extension",
    "description": "Slovenian language pack.",
    "keywords": ["locale"],
    "license": "MIT",
    "require": {
        "flarum/core": "^0.1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev",
            "master": "1.0"
        },
        "flarum-extension": {
            "title": "Slovenian",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "si",
            "title": "Slovenian"
        }
    }
}
